#!/usr/bin/perl

use warnings;
use strict;

{ local $/ ; $_ = <> };

my @data = <DATA>;
chomp @data;

print map "$_\n",
map {
    my @lines;
    
    map {
        my $index = $_;
        my $i = -1;
        map { $i ++; $lines[ $i ] .= substr $_, $index * 5, 5 } @data
    } m/\d/g;

    @lines
} split "\n"

__DATA__
